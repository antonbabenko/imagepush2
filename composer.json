{
    "name": "imagepush/imagepush.to",
    "description": "imagepush.to",
    "authors": [
        {
            "name": "Anton Babenko",
            "email": "anton@antonbabenko.com"
        },
        {
            "name": "Community contributions (anybody here? :))",
            "homepage": "https://github.com/antonbabenko/imagepush2/contributors"
        }
    ],
    
    "require": {
        "php":                           ">=5.4",

        "symfony/symfony":               "2.3.*@stable",
        "symfony/assetic-bundle":        "2.1.*",
        "symfony/monolog-bundle":        "2.3.*",
        "symfony/swiftmailer-bundle":    "2.3.*",
        "doctrine/orm":                  ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle":      "1.2.*",
        "twig/extensions":               "1.0.*",
        "sensio/distribution-bundle":    "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle":       "2.3.*",
        "jms/security-extra-bundle":     "1.5.*",
        "jms/di-extra-bundle":           "1.4.*",
        "jms/metadata":                  "1.3.*",
        "symfony/icu":                   "1.1.*",
        "doctrine/mongodb-odm-bundle":   "3.0.x-dev",
        "stof/doctrine-extensions-bundle": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "doctrine/doctrine-migrations-bundle": "dev-master",

        "incenteev/composer-parameter-handler": "~2.0",
        "oldsound/rabbitmq-bundle":      "1.0.0",
        "snc/redis-bundle":              "1.1.*",
        "liip/imagine-bundle":           "dev-master",
        "amazonwebservices/aws-sdk-for-php": "1.5.10",
        "knplabs/knp-gaufrette-bundle":  "0.1.4",
        "guzzle/guzzle":                 "dev-master",
        "ornicar/apc-bundle":            "*"

    },

    "require-dev": {
        "phpunit/phpunit":               "~3.7@stable",
        "fabpot/php-cs-fixer":           "0.3.*@dev",
        "fp/fumocker":                   "~1@stable",
        "makasim/doctrine-tester":       "dev-master"
    },

    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/makasim/doctrine-tester.git"
        }
    ],

    "minimum-stability": "dev",

    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },

    "config": {
        "bin-dir": "bin/",
        "process-timeout": 3600,
        "github-protocols": ["https"]
    },

    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "symlink",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "keep-outdated": true
        }
    }
}