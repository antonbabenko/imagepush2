{
    "name": "imagepush/imagepush.to",
    "description": "imagepush.to",
    "authors": [
    {
        "name": "Anton Babenko",
        "email": "anton@antonbabenko.com"
    }
    ],
    
    "require": {
        "php":                           ">=5.3.3",
        
        "symfony/symfony":               "2.0.15",
        "twig/twig":                     "1.8.3",
        "monolog/monolog":               "1.0.2",
        "doctrine/orm":                  "2.1.7",
        "symfony/doctrine-bridge":       "*",
        "doctrine/mongodb-odm":          "dev-fix/prepare-indexes",
		"doctrine/mongodb-odm-bundle":   "*",
        "swiftmailer/swiftmailer":       "4.1.7",
        "kriswallsmith/assetic":         "1.0.3",
        "twig/extensions":               "dev-master",
        "jms/metadata":                  "1.1.1",
        "sensio/framework-extra-bundle": "2.0.x-dev",
        "jms/security-extra-bundle":     "1.0.x-dev",
        "sensio/distribution-bundle":    "2.0.x-dev",
        "sensio/generator-bundle":       "2.0.x-dev",
        "symfony/twig-bundle":           "2.0.*",
        "symfony/assetic-bundle":        "2.0.x-dev",
        
        "snc/redis-bundle":              "2.0.x-dev",
        "predis/predis":                 "v0.7.x-dev",
        
        "liip/imagine-bundle":           "dev-master",
        
        "amazonwebservices/aws-sdk-for-php": "1.5.8.1",
        
        "knplabs/knp-gaufrette-bundle": "dev-master",
        
        "stof/doctrine-extensions-bundle": "1.0.x-dev",
        
        "digg/Services_Digg2": "*",
        "fabpot/Goutte": "*"
        
    },
    
    "require-dev": {
        "behat/behat":  "2.4.*@stable",
        "behat/mink":   "1.4.*@stable",

        "behat/mink-extension": "*",
        "behat/symfony2-extension": "*",
        "behat/mink-browserkit-driver":  "*",
        "behat/common-contexts": "*",
        "kriswallsmith/buzz": ">=0.5.0"
    },
    
    "repositories": [
    {
        "type": "package",
        "package": {
            "version": "dev-master",
            "name": "digg/Services_Digg2",
            "source": {
                "url": "https://github.com/digg/Services_Digg2.git",
                "type": "git",
                "reference": "master"
            },
            "dist":{
                "url":"https://github.com/digg/Services_Digg2/zipball/master",
                "type":"zip"
            }
        }
    },
    {
        "type": "package",
        "package": {
            "version": "dev-master",
            "name": "fabpot/Goutte",
            "source": {
                "url": "https://github.com/fabpot/Goutte.git",
                "type": "git",
                "reference": "master"
            },
            "dist":{
                "url":"https://github.com/fabpot/Goutte/zipball/master",
                "type":"zip"
            }
        }
    }

    ],
    
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    
    "config": {
        "bin-dir": "bin/"
    },
    
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}